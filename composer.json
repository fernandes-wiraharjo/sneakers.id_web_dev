{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "anlutro/l4-settings": "^1.0",
        "browner12/helpers": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hexters/ladmin": "^1.8",
        "intervention/image": "^2.7",
        "irfa/raja-ongkir": "^1.5",
        "jackiedo/log-reader": "^2.1",
        "kirschbaum-development/eloquent-power-joins": "^3.1",
        "laravel/framework": "^8.54",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.9",
        "nwidart/laravel-modules": "^8.2",
        "ramsey/uuid": "^4.7",
        "realrashid/sweet-alert": "^5.0",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-slack-alerts": "^1.2",
        "xendit/xendit-php": "^2.19",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-buttons": "^4.10",
        "yajra/laravel-datatables-oracle": "^9.19"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/ThemeHelper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
